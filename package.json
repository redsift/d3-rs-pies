{
  "name": "@redsift/d3-rs-pies",
  "version": "0.3.0",
  "description": "Generates pie charts using D3v4.",
  "keywords": [
    "pie",
    "redsift",
    "d3",
    "chart"
  ],
  "homepage": "https://github.com/redsift/d3-rs-pies",
  "license": "MIT",
  "author": {
    "name": "Rahul Powar",
    "url": "https://medium.com/@rahulpowar"
  },
  "main": "distribution/d3-rs-pies.umd-es2015.js",
  "browser": "distribution/d3-rs-pies.umd-es2015.min.js",
  "jsnext:main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redsift/d3-rs-pies.git"
  },
  "scripts": {
    "serve": "gulp -o d3-rs-pies -g d3-selection -g d3-shape serve",
    "prepublish": "npm run clean; npm run build",
    "build": "npm run clean; node ./node_modules/@redsift/rollup-bundler/src/index.js",
    "clean": "rm -rf dist",
    "pretest": "npm outdated || true && npm run build",
    "test": "tape 'test/**/*-test.js' && eslint index.js src"
  },
  "dependencies": {
    "d3-selection": "^1.0.0",
    "d3-shape": "^1.0.0",
    "d3-format": "^1.0.0",
    "d3-time-format": "^2.0.0",
    "d3-interpolate": "^1.1.0",
    "@redsift/d3-rs-svg": "~0.7.0",
    "@redsift/d3-rs-theme": "~0.6.0",
    "@redsift/d3-rs-tip": "~0.10.0",
    "@redsift/d3-rs-legends": "~0.4.0",
    "@redsift/d3-rs-intl": "~0.3.0"
  },
  "devDependencies": {
    "@redsift/rollup-bundler": "^2.0.2",
    "@redsift/tape-reel": "~0.1.1",
    "browser-sync": "^2.13.0",
    "del": "^2.2.1",
    "eslint": "^3.1.1",
    "gulp": "4.0.2",
    "gulp-rename": "2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "3.0.2",
    "json2module": "~0.0.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "yargs": "^4.8.1"
  },
    "overrides": {
    "lodash": "4.17.21",
    "lodash.template": "4.5.0",
    "chokidar": "3.5.3",
    "glob-parent": "6.0.2"
  }
}

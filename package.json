{
  "name": "@redsift/d3-rs-pies",
  "version": "0.2.1",
  "description": "Generates pie charts using D3v4.",
  "keywords": [
    "pie",
    "redsift",
    "d3",
    "chart"
  ],
  "homepage": "https://github.com/redsift/d3-rs-pies",
  "license": "MIT",
  "author": {
    "name": "Rahul Powar",
    "url": "https://medium.com/@rahulpowar"
  },
  "module": "dist/d3-rs-pies.esm.js",
  "jsnext:main": "dist/d3-rs-pies.esm.js",
  "main": "dist/d3-rs-pies.umd.min.js",
  "browser": "dist/d3-rs-pies.umd.min.js",
  "esnext": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redsift/d3-rs-pies.git"
  },
  "scripts": {
    "serve": "gulp -o d3-rs-pies -g d3-selection -g d3-shape serve",
    "prepublish": "npm run clean; npm run build",
    "build": "npm run clean; node ./node_modules/@redsift/rollup-bundler/src/index.js bundle.config.js",
    "clean": "rm -rf dist",
    "pretest": "npm outdated || true && npm run build",
    "test": "tape 'test/**/*-test.js' && eslint index.js src"
  },
  "dependencies": {
    "@redsift/d3-rs-intl": "^0.0.2",
    "@redsift/d3-rs-legends": "^0.1.0",
    "@redsift/d3-rs-svg": "^0.5.2",
    "@redsift/d3-rs-theme": "^0.4.1",
    "@redsift/d3-rs-tip": "^0.3.2",
    "d3-format": "^1.0.0",
    "d3-interpolate": "^1.1.0",
    "d3-selection": "^1.0.0",
    "d3-shape": "^1.0.0",
    "d3-time-format": "^2.0.0"
  },
  "devDependencies": {
    "@redsift/rollup-bundler": "^0.4.0",
    "@redsift/tape-reel": "^0.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.13.0",
    "del": "^2.2.1",
    "eslint": "^3.1.1",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "json2module": "~0.0.3",
    "rollup": "^0.34.1",
    "rollup-plugin-buble": "^0.12.1",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-json": "^2.0.0",
    "rollup-plugin-node-resolve": "^1.5.0",
    "rollup-stream": "^1.11.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^4.8.1"
  }
}
